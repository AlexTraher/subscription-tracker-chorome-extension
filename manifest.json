{
  "name": "Subscription tracker",
  "version": "1.0",
  "description": "Keeps a track of subscriptions you use, and those you don't",
  "background": {
    "page": "background.html",
    "persistant": false
  },
  "permissions": [
    "storage",
    "declarativeContent"
  ],
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}